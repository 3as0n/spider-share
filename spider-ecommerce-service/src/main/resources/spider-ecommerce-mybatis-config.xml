<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeHandlers>
        <typeHandler javaType="java.util.concurrent.atomic.AtomicInteger"
                     handler="com.datatrees.spider.share.service.typehandler.AtomicIntegerTypeHander"/>
        <typeHandler javaType="java.util.concurrent.atomic.AtomicLong"
                     handler="com.datatrees.spider.share.service.typehandler.AtomicLongTypeHander"/>
    </typeHandlers>

</configuration>