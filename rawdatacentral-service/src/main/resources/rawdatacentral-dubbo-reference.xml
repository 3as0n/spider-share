<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="rawdatacentral"/>

    <dubbo:monitor protocol="registry"/>

    <!-- 使用multicast广播注册中心暴露发现服务地址 -->
    <dubbo:registry address="${dubbo.zookeeper.address}" file="${user.home}/.dubbo/rawdatacentral.cache"/>

    <dubbo:protocol name="dubbo" port="20192"/>


    <!--代理-->
    <dubbo:reference interface="com.treefinance.proxy.api.ProxyProvider" id="proxyProvider" check="false"/>
    <dubbo:reference interface="com.datatrees.notify.sms.newservice.SmsNewService" id="smsNewService" check="false"/>
    <!--获取appId服务-->
    <dubbo:reference interface="com.treefinance.saas.grapserver.facade.service.TaskFacade" id="taskFacade" check="false"/>
    <!--获取商户列表-->
    <dubbo:reference interface="com.treefinance.saas.merchant.center.facade.service.MerchantBaseInfoFacade" id="merchantBaseInfoFacade" version="1.0.0" check="false"/>
</beans>


