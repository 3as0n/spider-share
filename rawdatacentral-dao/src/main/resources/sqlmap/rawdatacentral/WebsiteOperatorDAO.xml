<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.datatrees.rawdatacentral.dao.WebsiteOperatorqueryMaxWeightWebsiteDAO">


    <select id="queryMaxWeightWebsite" resultType="com.datatrees.rawdatacentral.domain.model.WebsiteOperator">
        select * from `website_operator` where website_name = #{websiteName} order by weight desc limit 1;
    </select>


</mapper>
