<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.datatrees.rawdatacentral.dao.OperatorGroupDAO">

    <select id="queryMaxWeightWebsite" resultType="com.datatrees.rawdatacentral.domain.model.OperatorGroup">
        select * from operator_group where group_code = #{groupCode} order by weight desc limit 1;
    </select>

</mapper>