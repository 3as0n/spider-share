<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="Ecommerce">
	<resultMap class="Ecommerce" id="RM-Ecommerce">
		<result column="Id" property="id" />
		<result column="WebsiteId" property="websiteId" />
		<result column="EcommerceName" property="ecommerceName" />
		<result column="IsEnabled" property="isEnabled" />
	</resultMap>

	<select id="getAllEcommerce" resultMap="RM-Ecommerce">
		SELECT * FROM T_ECOMMERCE WHERE IsEnabled='true'
	</select>

</sqlMap>
	