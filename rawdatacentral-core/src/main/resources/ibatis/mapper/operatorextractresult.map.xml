<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="OperatorExtractResult">
	<resultMap id="BaseResultMap" class="OperatorExtractResult">
		<result column="Id" property="id" jdbcType="INTEGER" />
		<result column="TaskLogId" property="taskId" jdbcType="INTEGER" />
		<result column="WebsiteId" property="websiteId" jdbcType="INTEGER" />
		<result column="OperatorId" property="operatorId" jdbcType="INTEGER" />
		<result column="Status" property="status" jdbcType="TINYINT" />
		<result column="Remark" property="remark" jdbcType="VARCHAR" />
		<result column="Url" property="url" jdbcType="VARCHAR" />
		<result column="UniqueSign" property="uniqueSign" jdbcType="VARCHAR" />
		<result column="UniqueMd5" property="uniqueMd5" jdbcType="CHAR" />
		<result column="ResultType" property="resultType" jdbcType="VARCHAR" />
		<result column="StoragePath" property="storagePath" jdbcType="VARCHAR" />
		<!--<result column="CreatedAt" property="createdat" jdbcType="TIMESTAMP" /> -->
		<!--<result column="UpdatedAt" property="updatedat" jdbcType="TIMESTAMP" /> -->
	</resultMap>
	<sql id="Base_Column_List">
		Id, TaskId, WebsiteId, OperatorId, Status, Remark, Url, UniqueSign, ResultType,
		StoragePath, CreatedAt, UpdatedAt
	</sql>
	<select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterClass="java.lang.Integer">
		select
		<include refid="Base_Column_List" />
		from t_operator_extract_result
		where Id = #id#
	</select>
	<delete id="deleteByPrimaryKey" parameterClass="java.lang.Integer">
		delete from t_operator_extract_result
		where Id = #id#
	</delete>
	<insert id="insert" parameterClass="OperatorExtractResult">
		insert into t_operator_extract_result (Id, TaskId, Duration,
		WebsiteId, OperatorId, Status,
		Remark, Url, UniqueSign,UniqueMd5,
		ResultType, StoragePath, CreatedAt)
		values (#id#, #taskId#, #duration#,
		#websiteId#, #operatorId#, #status#,
		#remark#, #url#, #uniqueSign#,#uniqueMd5#,
		#resultType#, #storagePath#, now())
		<selectKey resultClass="int" keyProperty="id">
			SELECT @@IDENTITY AS id
		</selectKey>
	</insert>
</sqlMap>