	</resultMap>
	<sql id="Base_Column_List">
		Id, TaskLogId, WebsiteId, EcommerceId, Status, Remark, Url, UniqueSign, ResultType,
		StoragePath, CreatedAt, UpdatedAt
	</sql>
	<select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterClass="java.lang.Integer">
		select
		<include refid="Base_Column_List" />
		from t_ecommerce_extract_result
		where Id = #id#
	</select>
	<delete id="deleteByPrimaryKey" parameterClass="java.lang.Integer">
		delete from t_ecommerce_extract_result
		where Id = #id#
	</delete>
