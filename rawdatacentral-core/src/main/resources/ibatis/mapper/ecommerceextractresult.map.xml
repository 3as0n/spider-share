		WebsiteId, EcommerceId, Status,
		Remark, Url, UniqueSign,UniqueMd5,
		ResultType, StoragePath, CreatedAt)
		values (#id#, #taskId#,
		#websiteId#, #ecommerceId#, #status#,
		#remark#, #url#, #uniqueSign#,#uniqueMd5#,
		#resultType#, #storagePath#, now())
		<selectKey resultClass="int" keyProperty="id">
			SELECT @@IDENTITY AS id
		</selectKey>
	</insert>
	<update id="updateByPrimaryKey" parameterClass="EcommerceExtractResult">
		update t_ecommerce_extract_result
		set TaskLogId = #taskId#,
		WebsiteId = #websiteId#,
		EcommerceId = #ecommerceId#,
		Status = #status#,
		Remark = #remark#,
		Url = #url#,
		UniqueSign = #uniqueSign#,
		ResultType = #resultType#,
		StoragePath = #storagePath#,
		UpdatedAt = now()
		where Id = #id#
	</update>
