<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="Operator">
	<resultMap class="Operator" id="RM-Operator">
		<result column="Id" property="id" />
		<result column="WebsiteId" property="websiteId" />
		<result column="OperatorName" property="operatorName" />
		<result column="Region" property="region" />
		<result column="IsEnabled" property="isEnabled" />
	</resultMap>

	<select id="getAllOperator" resultMap="RM-Operator">
		SELECT * FROM T_OPERATOR WHERE IsEnabled='true'
	</select>

</sqlMap>
	