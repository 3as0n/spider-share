<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:provider timeout="30000" retries="3"/>

    <!--注册到dubbo -->
    <dubbo:service interface="com.datatrees.rawdatacentral.api.CrawlerService" ref="crawlerServiceImpl"/>
    <dubbo:service interface="com.datatrees.rawdatacentral.api.CrawlerTaskService" ref="crawlerTaskServiceImpl"/>
    <dubbo:service interface="com.datatrees.rawdatacentral.api.WebsiteGroupServiceApi" ref="websiteGroupServiceApiImpl"/>
    <!--运营商-->
    <dubbo:service interface="com.datatrees.rawdatacentral.api.CrawlerOperatorService"
                   ref="crawlerOperatorServiceImpl"/>
    <dubbo:service interface="com.datatrees.rawdatacentral.api.WebsiteOperatorServiceApi"
                   ref="websiteOperatorServiceApiImpl"/>
    <!--学信网-->
    <dubbo:service interface="com.datatrees.rawdatacentral.api.RpcEducationService" ref="rpcEducationServiceImpl"/>

</beans>


