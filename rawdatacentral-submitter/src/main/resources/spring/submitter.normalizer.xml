<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="submitDataNormalizerList" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="mailBillSubmitDataNormalizer" />
				<ref bean="ebankSubmitDataNormalizer" />
				<ref bean="ecommerceSubmitDataNormalizer" />
				<ref bean="operatorSubmitDataNormalizer" />
				<ref bean="defaultSubmitDataNormalizer" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="submitNormalizerFactory" class="com.datatrees.rawdatacentral.core.common.NormalizerFactory">
		<property name="normalizerList" ref="submitDataNormalizerList" />
	</bean>

</beans>