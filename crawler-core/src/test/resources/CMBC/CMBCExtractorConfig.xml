<?xml version="1.0" encoding="UTF-8"?>
<config>
	<extrator-selectors>
		<selector field="pageContent" contain="民生信用卡" dis-contain="" ref="page-extractor_1">
		</selector>
		<selector field="pageContent" contain="账单与您实际交易不符" dis-contain="" ref="page-extractor_bufa">
		</selector>
		<selector field="pageContent" contain="x_fixBand1" dis-contain="" ref="page-extractor_2">
		</selector>
	</extrator-selectors>

	<page-extractor-definition>
		<page-extractor id="page-extractor_bufa">
			<page-sources>
				<source field="pageContent">
				</source>
			</page-sources>
			<object-segment name="bankBills">
				<field-extractor id="baseString" field="temp">
					<operation type="xpath"><![CDATA[/text(###)]]></operation>
					<operation from="&amp;nbsp;" to="" type="replace"></operation>
					<operation from=" " to="" type="replace"></operation>
				</field-extractor>
				<field-extractor id="dateTemp" field="temp" result-type="string">
					<operation type="xpath"><![CDATA[span:contains(年):contains(月):not(:has(span))/text()]]></operation>
				</field-extractor>
				<field-extractor id="field_bill_month" source="dateTemp" field="BillMonth" result-type="date" format="yyyy年MM月">
					<operation type="return"></operation>
				</field-extractor>
				<field-extractor id="field_bill_month" field="BillMonth" source="baseString" result-type="date" format="yyyy年MM月" stand-by="true">
					<operation type="regex" index="1"><![CDATA[###([年月\d]*)###份电子对账单]]></operation>
				</field-extractor>
				<field-extractor id="field_bill_month" field="BillMonth" source="baseString" result-type="date" format="yyyy/MM" stand-by="true" not-empty="true">
					<operation type="regex" index="1"><![CDATA[(\d{4}/\d{2}/\d{2})###月份电子对账单]]></operation>
				</field-extractor>
				<field-extractor id="Year" source="dateTemp" field="temp" field-visible-type="request">
					<operation type="regex" index="1"><![CDATA[(\d{4})年]]></operation>
				</field-extractor>
				<field-extractor id="Year" source="baseString" field="temp" field-visible-type="request" stand-by="true">
					<operation type="regex" index="1"><![CDATA[民生信用卡账户###(\d{4})]]></operation>
				</field-extractor>
				<!--姓名 非空格和一个特殊白字符 -->
				<field-extractor id="field_name" field="NameOnCard" source="baseString" result-type="string" not-empty="true">
					<operation type="regex" index="1"><![CDATA[###([^#]*)(先生|女士)：###]]></operation>
				</field-extractor>

				<!-- 账单日 -->
				<field-extractor id="field_bill_date" field="BillDate" result-type="date" format="yyyy/MM/dd">
					<operation type="xpath"><![CDATA[span:contains(本期账单日):not(:has(span))/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[([\d/]+)]]></operation>
				</field-extractor>
				<field-extractor id="field_bill_date" field="BillDate" source="baseString" result-type="date" format="yyyy/MM/dd" stand-by="true">
					<operation type="regex" index="1"><![CDATA[(\d{4}/\d{2}/\d{2})###月份电子对账单]]></operation>
				</field-extractor>
				<field-extractor id="field_payment_due" field="PaymentDueDate" result-type="date" format="yyyyMMdd">
					<operation type="xpath"><![CDATA[span:contains(最后还款日):not(:has(span))/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[([\d/]+)]]></operation>
				</field-extractor>
				<field-extractor id="field_payment_due" field="PaymentDueDate" result-type="date" format="yyyy/MM/dd" stand-by="true">
					<operation type="xpath"><![CDATA[span:contains(最后还款日):not(:has(span))/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[([\d/]+)]]></operation>
				</field-extractor>

				<object-segment name="MinPayment" type="xpath" value="span:contains(人民币账户):contains(本期应还款金额):not(:has(span:contains(人民币账户):contains(本期应还款金额)))" result-class="currency_payment">
					<field-extractor id="MinPayment" field="MinPayment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[font:contains(最低还款额):not(:has(font))/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="NewBalance" type="xpath" value="span:contains(人民币账户):contains(本期应还款金额):not(:has(span:contains(人民币账户):contains(本期应还款金额)))" result-class="currency_payment">
					<field-extractor id="NewBalance" field="NewBalance" result-type="currency_payment">
						<operation type="xpath"><![CDATA[font:contains(本期应还款金额):not(:has(font))/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="FCMinPayment" type="xpath" value="span:contains(美元账户):contains(本期应还款金额):not(:has(span:contains(美元账户):contains(本期应还款金额)))" result-class="currency_payment">
					<field-extractor id="FCMinPayment" field="FCMinPayment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[font:contains(最低还款额):not(:has(font))/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,]+)]]></operation>
						<operation type="append" index="0"><![CDATA[美元]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="FCNewBalance" type="xpath" value="span:contains(美元账户):contains(本期应还款金额):not(:has(span:contains(美元账户):contains(本期应还款金额)))" result-class="currency_payment">
					<field-extractor id="FCNewBalance" field="FCNewBalance" result-type="currency_payment">
						<operation type="xpath"><![CDATA[font:contains(本期应还款金额):not(:has(font))/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,]+)]]></operation>
						<operation type="append" index="0"><![CDATA[美元]]></operation>
					</field-extractor>
				</object-segment>

				<field-extractor id="baseStringMather" field="temp" field-visible-type="request">
					<operation type="xpath"><![CDATA[tr:has(table:contains(人民币账户):contains(本期应还款金额):not(:has(table))):not(:contains(本期账单日))+tr]]></operation>
				</field-extractor>

				<object-segment name="LastBalance" type="xpath" source="baseStringMather" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="LastBalance" field="LastBalance" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[3]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="NewCharges" type="xpath" source="baseStringMather" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="NewCharges" field="NewCharges" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[7]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="LastPayment" type="xpath" source="baseStringMather" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="LastPayment" field="LastPayment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[5]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="LastPayment" result-class="currency_payment" stand-by="true">
					<field-extractor id="LastPayment" field="LastPayment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[span:contains(本期已还金额):not(:has(span))/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="NewCharges" result-class="currency_payment" stand-by="true">
					<field-extractor id="AdvancedCash" field="temp" result-type="number">
						<operation type="xpath"><![CDATA[span:contains(本期预借现金金额):not(:has(span))/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>
					<field-extractor id="PurchaseTemp" field="temp" result-type="number">
						<operation type="xpath"><![CDATA[span:contains(本期消费金额):not(:has(span))/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>

					<field-extractor id="NewCharges" field="NewCharges" result-type="currency_payment">
						<operation type="calculate"><![CDATA[${AdvancedCash}+${PurchaseTemp}]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="Adjustment" type="xpath" source="baseStringMather" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="Adjustment" field="Adjustment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[9]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="Interest" type="xpath" source="baseStringMather" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="Interest" field="Interest" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[11]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
					</field-extractor>
				</object-segment>

				<field-extractor id="baseStringMatherFC" field="temp" field-visible-type="request">
					<operation type="xpath"><![CDATA[tr:has(table:contains(美元账户):contains(本期应还款金额):not(:has(table))):not(:contains(本期账单日))+tr]]></operation>
				</field-extractor>

				<object-segment name="FCLastBalance" type="xpath" source="baseStringMatherFC" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))"
					result-class="currency_payment">
					<field-extractor id="FCLastBalance" field="FCLastBalance" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[3]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
						<operation type="append" index="0"><![CDATA[美元]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="FCNewCharges" type="xpath" source="baseStringMatherFC" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="FCNewCharges" field="FCNewCharges" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[7]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
						<operation type="append" index="0"><![CDATA[美元]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="FCLastPayment" type="xpath" source="baseStringMatherFC" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))"
					result-class="currency_payment">
					<field-extractor id="FCLastPayment" field="FCLastPayment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[5]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
						<operation type="append" index="0"><![CDATA[美元]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="FCAdjustment" type="xpath" source="baseStringMatherFC" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="FCAdjustment" field="FCAdjustment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[9]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
						<operation type="append" index="0"><![CDATA[美元]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="FCInterest" type="xpath" source="baseStringMatherFC" value="table:contains(上期账单金额):contains(本期已还金额):not(:has(table)) tr:not(:contains(上期账单金额))" result-class="currency_payment">
					<field-extractor id="FCInterest" field="FCInterest" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//td[11]/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[([\d\.,-]+)]]></operation>
						<operation type="append" index="0"><![CDATA[美元]]></operation>
					</field-extractor>
				</object-segment>

				<object-segment name="ShoppingSheet" type="xpath" value="table:contains(交易日):contains(交易摘要):not(:has(table)) tr:not(:contains(交易日))">
					<field-extractor id="ShoppingSheetMatcher" field="temp">
						<operation type="xpath"><![CDATA[/text(###)]]></operation>
						<operation type="replace" from=" " to=""></operation>
						<operation type="regex"><![CDATA[(\d{2}/\d{2})(###\d{4}/\d{2}/\d{2})?###([^#]*)(###[^#]*)?###([\d\.,-]*)###(\d+)###]]></operation>
					</field-extractor>
					<field-extractor id="field_Currency" field="CurrencyType" result-type="currency" field-visible-type="request">
						<operation type="matchgroup" source="ShoppingSheetMatcher" index="4"></operation>
					</field-extractor>
					<field-extractor id="field_Currency" field="CurrencyType" result-type="currency" field-visible-type="request" stand-by="true">
						<operation type="set"><![CDATA[RMB]]></operation>
					</field-extractor>
					<field-extractor id="field-extractor_1" field="TransDate" result-type="date" format="yyyy/MM/dd">
						<operation type="matchgroup" source="ShoppingSheetMatcher" index="1"></operation>
						<operation type="append" index="0">${Year}/</operation>
					</field-extractor>
					<field-extractor id="field-extractor_2" field="PostDate" result-type="date" format="yyyy/MM/dd">
						<operation type="matchgroup" source="ShoppingSheetMatcher" index="2"></operation>
						<operation type="replace" from="###" to=""></operation>
					</field-extractor>
					<field-extractor id="field-extractor_3" field="Description" result-type="string">
						<operation type="matchgroup" source="ShoppingSheetMatcher" index="3"></operation>
						<operation from="&amp;nbsp;" to="" type="replace"></operation>
					</field-extractor>
					<field-extractor id="field-extractor_4" field="AmountMoney" result-type="currency_payment">
						<operation type="matchgroup" source="ShoppingSheetMatcher" index="5"></operation>
						<operation type="append" index="0"><![CDATA[${field_Currency}]]></operation>
					</field-extractor>
					<field-extractor id="field-extractor_5" field="CardNo" result-type="string">
						<operation type="matchgroup" source="ShoppingSheetMatcher" index="6"></operation>
					</field-extractor>
				</object-segment>
			</object-segment>
		</page-extractor>

		<page-extractor id="page-extractor_1">
			<page-sources>
				<source field="pageContent">
				</source>
			</page-sources>
			<object-segment name="bankBills">
				<field-extractor id="DueDateTemp" field="temp">
					<operation type="xpath">
                        <![CDATA[//span[@id='fixBand1']//span[@id='fixBand6']//span[@id='fixBand9']]]></operation>
				</field-extractor>
				<field-extractor id="DueDateTemp" field="temp" stand-by="true">
					<operation type="xpath">
                        <![CDATA[//span[@id='fixBand5']//span[@id='fixBand6']//span[@id='fixBand9']]]></operation>
				</field-extractor>
				<field-extractor id="IntegralTemp" field="temp">
					<operation type="xpath">
                        <![CDATA[table:contains(积分明细):contains(积分余额):not(:has(table:contains(积分明细):contains(积分余额)))]]></operation>
				</field-extractor>
				<field-extractor id="BillMonthTemp" field="temp">
					<operation type="xpath">
                        <![CDATA[//span[@id='fixBand1']//span[@id='regionFrame1']//span[@id='fixBand1']/table/tbody/tr[2]//td[4]/text()]]></operation>
				</field-extractor>
				<field-extractor id="BillMonthTemp" field="temp" stand-by="true">
					<operation type="xpath"><![CDATA[/text(###)]]></operation>
					<operation from="&amp;nbsp;" to="" type="replace"></operation>
					<operation from=" " to="" type="replace"></operation>
					<operation type="regex" index="1"><![CDATA[([年月\d]*)(电子)?对账单]]></operation>
				</field-extractor>
				<field-extractor id="BillMonthTemp" field="temp" stand-by="true">
					<operation type="xpath"><![CDATA[/text()]]></operation>
					<operation from="&amp;nbsp;" to="" type="replace"></operation>
					<operation from=" " to="" type="replace"></operation>
					<operation type="regex" index="1"><![CDATA[([年月\d]{8})\s*(电子)?对账单]]></operation>
				</field-extractor>
				<field-extractor id="Year" source="BillMonthTemp" field="temp" field-visible-type="request">
					<operation type="regex" index="1"><![CDATA[(\d{4})年]]></operation>
				</field-extractor>

				<!--姓名 非空格和一个特殊白字符 -->
				<field-extractor id="field_name" field="NameOnCard" result-type="string">
					<operation type="xpath">
                        <![CDATA[//span[@id='fixBand1']//span[@id='regionFrame1']//span[@id='fixBand1']//span[@id='fixBand2']/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[尊敬的([^&  ]*)]]></operation>
				</field-extractor>
				<field-extractor id="field_name" field="NameOnCard" result-type="string" stand-by="true" >
					<operation type="xpath">/body/text()</operation>
					<operation type="replace" from="&amp;nbsp;" to=""/>
					<operation type="regex" index="1"><![CDATA[尊敬的(.*)\s*(?:先生|女士)(?:：|:)]]></operation>
				</field-extractor>
				<field-extractor id="field_name" field="NameOnCard" result-type="string" stand-by="true" not-empty="true">
					<operation type="regex" index="1"><![CDATA[尊敬的([^&  ]*)]]></operation>
				</field-extractor>

				<field-extractor id="field_bill_month" source="BillMonthTemp" field="BillMonth" result-type="date" format="yyyy年MM月" not-empty="true">
					<operation type="return" />
				</field-extractor>
				<!-- 账单日 -->
				<field-extractor id="field_bill_date" field="BillDate" source="DueDateTemp" result-type="date" format="yyyy/MM/dd">
					<operation type="xpath"><![CDATA[//span[@id='fixBand7']//td[3]/div/text()]]></operation>
				</field-extractor>
				<field-extractor id="field_payment_due" field="PaymentDueDate" source="DueDateTemp" result-type="date" format="yyyy/MM/dd">
					<operation type="xpath"><![CDATA[//span[@id='fixBand8']//td[3]/div/text()]]></operation>
				</field-extractor>

				<object-segment name="Integral" source="IntegralTemp" type="xpath" value="//span[@id='fixBand12']//tr[3]/text()" result-class="number">
					<field-extractor id="Integral" field="Integral" result-type="number">
						<operation type="return"></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="IntegralAdd" source="IntegralTemp" type="xpath" value="//span[@id='fixBand16']//tr[3]/text()" result-class="number">
					<field-extractor id="IntegralAdd" field="IntegralAdd" result-type="number">
						<operation type="return"></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="IntegralUsed" source="IntegralTemp" type="xpath" value="//span[@id='fixBand17']//tr[3]/text()" result-class="number">
					<field-extractor id="IntegralUsed" field="IntegralUsed" result-type="number">
						<operation type="return"></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="IntegralAdjust" source="IntegralTemp" type="xpath" value="//span[@id='fixBand19']//tr[3]/text()" result-class="number">
					<field-extractor id="IntegralAdjust" field="IntegralAdjust" result-type="number">
						<operation type="return"></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="IntegralReward" source="IntegralTemp" type="xpath" value="//span[@id='fixBand20']//tr[3]/text()" result-class="number">
					<field-extractor id="IntegralReward" field="IntegralReward" result-type="number">
						<operation type="return"></operation>
					</field-extractor>
				</object-segment>

				<field-extractor id="loopTemp" field="temp" field-visible-type="request">
					<operation type="xpath"><![CDATA[span#loopBand2]]></operation>
				</field-extractor>
				<object-segment name="MinPayment" source="loopTemp" type="xpath" value="tr:contains(Min.Payment):contains(人民币):not(:has(tr))" result-class="currency_payment">
					<field-extractor id="MinPayment" field="MinPayment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[.*(人民币.*[\d,.-]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="FCMinPayment" source="loopTemp" type="xpath" value="tr:contains(Min.Payment):contains(美元):not(:has(tr))" result-class="currency_payment">
					<field-extractor id="FCMinPayment" field="FCMinPayment" result-type="currency_payment">
						<operation type="xpath"><![CDATA[/text()]]></operation>
						<operation type="regex" index="1"><![CDATA[.*(美元.*[\d,.-]+)]]></operation>
					</field-extractor>
				</object-segment>
				<object-segment name="NewBalance" source="loopTemp" type="xpath" value="tr:contains(Min.Payment):contains(人民币) + tr span#fixBand24" result-class="currency_payment">
					<field-extractor id="NewBalance" field="NewBalance" result-type="currency_payment">
						<operation type="xpath"><![CDATA[//span[@id='fixBand24']//span[@id='fixBand7']/table/tbody/tr[3]/text()]]></operation>
						<operation type="append" index="-1"><![CDATA[人民币]]></operation>
					</field-extractor>
				</object-segment>
