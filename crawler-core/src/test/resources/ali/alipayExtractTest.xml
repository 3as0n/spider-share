<?xml version="1.0" encoding="UTF-8"?>
<config>
	<extrator-selectors>
		<selector field="url" contain="waimai.taobao|wuliu.taobao|trade_item_detail.htm|orderDetail.htm" ref="page-tradeAddress_page"></selector>
		<selector field="url" contain="detail/orderDetail.htm" ref="page-tradeAddress_tmallpage"></selector>
		<selector field="url" contain="portal/i.htm" dis-contain="" ref="page-extractor_baseInfo"></selector>
		<selector field="url" contain="queryTradeDetail.htm" dis-contain="" ref="page-extractor_detailInfo_containaddress"></selector>
		<selector field="url" contain="record/.*beginDate" dis-contain="download.htm" ref="page-extractor_detailInfo"></selector>
		<selector field="url" contain="chargeRemindInfoEntering.htm" dis-contain="" ref="page-extractor_jiaoFeiInfo"></selector>
		<selector field="url" contain="asset/bindQuery.json" dis-contain="" ref="page-extractor_bankCardInfo"></selector>
		<selector field="url" contain="member/fresh/deliver_address" dis-contain="" ref="page-extractor_deliverAddress"></selector>
		<selector field="url" contain="trade/itemlist/asyncBought" dis-contain="_taoignore_address" ref="page-taobao_record"></selector>
		<selector field="url" contain="personal/sysmsg/list.json" dis-contain="" ref="page-zmCredit"></selector>
		<selector field="url" contain="deliveraddress/index.htm" dis-contain="" ref="page-alipay_deliverAddress"></selector>
		<selector field="url" contain="alipay.com/record/download.htm" dis-contain="" ref="page-extractor_jsonRecord"></selector>
	</extrator-selectors>

	<page-extractor-definition>
		<page-extractor id="page-extractor_jsonRecord" dis-alternative="true">
			<page-sources>
				<source field="pageContent"></source>
			</page-sources>
			<object-segment name="trades" type="jsonpath" value="$.[*]">
				<field-extractor id="tradeDetailUrl" field="tradeDetailUrl" result-type="string">
					<operation type="jsonpath">$.tradeDetailUrl</operation>
				</field-extractor>
				<field-extractor id="title" field="title" result-type="string" not-empty="true">
					<operation type="jsonpath">$.title</operation>
				</field-extractor>
				<field-extractor id="tradeNumber" field="tradeNumber" result-type="string">
					<operation type="jsonpath">$.tradeNumber</operation>
				</field-extractor>
				<field-extractor id="uniqueSign" field="uniqueSign" result-type="string">
					<operation type="template">${tradeNumber}</operation>
				</field-extractor>
				<field-extractor id="tradeDatetime" field="tradeDatetime" result-type="date" format="yyyy-MM-dd HH:mm:ss">
					<operation type="jsonpath">$.tradeDatetime</operation>
				</field-extractor>
				<field-extractor id="otherSide" field="otherSide" result-type="string">
					<operation type="jsonpath">$.otherSide</operation>
					<operation type="decode" decode-type="standard"></operation>
				</field-extractor>
				<field-extractor id="tradeType" field="tradeType" result-type="int" default-value="0">
					<operation type="jsonpath">$.tradeType</operation>
				</field-extractor>
				<field-extractor id="tradeStatus" field="tradeStatus" result-type="int" default-value="-1">
					<operation type="jsonpath">$.tradeStatus</operation>
					<operation type="mapping" group-name="tradeStatus"></operation>
				</field-extractor>
				<field-extractor id="amount" field="amount" result-type="number">
					<operation type="jsonpath">$.amount</operation>
				</field-extractor>
			</object-segment>
		</page-extractor>
		<page-extractor id="page-alipay_deliverAddress" dis-alternative="true">
			<page-sources>
				<source field="pageContent"></source>
			</page-sources>

			<object-segment name="address" type="xpath" value="div.am-list-item">
				<field-extractor id="pageConent" field="temp">
					<operation type="return"></operation>
				</field-extractor>
				<field-extractor id="pageSource" field="temp">
					<operation type="xpath">span.J-fullname/text()</operation>
					<operation type="triple">${this}=?${pageConent}未命名过滤:${pageConent}</operation>
				</field-extractor>
				<object-segment name="address" source="pageSource" dis-contains="未命名过滤">
					<field-extractor id="receiveName" field="receiveName" result-type="string" not-empty="true">
						<operation type="xpath">span.J-fullname/text()</operation>
					</field-extractor>
					<field-extractor id="region" field="region" result-type="string" not-empty="true">
						<operation type="xpath">div.J-address</operation>
						<operation type="xpath">//@data-prov</operation>
					</field-extractor>
					<field-extractor id="detailedAddress" field="detailedAddress" result-type="string" not-empty="true">
						<operation type="xpath">div.J-address/text()</operation>
						<operation type="trim"></operation>
					</field-extractor>
					<field-extractor id="postcode" field="postcode" result-type="string">
						<operation type="xpath">span.J-post/text()</operation>
					</field-extractor>
					<field-extractor id="telNumber" field="telNumber" result-type="string">
						<operation type="xpath">span.J-mobilePhone/text()</operation>
					</field-extractor>
				</object-segment>
			</object-segment>
		</page-extractor>
		<page-extractor id="page-zmCredit" dis-alternative="true">
			<page-sources>
				<source field="pageContent"></source>
			</page-sources>
			<object-segment name="zmCredit">
				<!-- <field-extractor id="evaluationTime" field="evaluationTime" result-type="date" format="yyyy.MM.dd">
					<operation type="regex" index="2"><![CDATA[你现在芝麻分为(\d+)，评估时间([\d\.]+)]]></operation>
				</field-extractor>
				<field-extractor id="creditScore" field="creditScore" stand-by="true" result-type="int">
					<operation type="regex" index="1"><![CDATA[你现在芝麻分为(\d+)，评估时间]]></operation>
				</field-extractor> -->
				<field-extractor id="evaluationTime" field="evaluationTime" result-type="date" format="yyyy.MM.dd">
					<operation type="datetime" base-type="firstdayofthismonth" format="yyyy.MM.dd"></operation>
				</field-extractor>
				<field-extractor id="creditScore" field="creditScore" result-type="int">
					<operation type="template"><![CDATA[${creditScore}]]></operation>
				</field-extractor>
			</object-segment>
		</page-extractor>

		<page-extractor id="page-tradeAddress_tmallpage" dis-alternative="true">
			<page-sources>
				<source field="pageContent">
					<regex index="1"><![CDATA[detailData\s*=\s*(((?!</script>).)*)]]></regex>
				</source>
			</page-sources>
			<object-segment name="tradeAddress">
				<field-extractor id="tradeNumber" field="tradeNumber" stand-by="true" result-type="string">
					<operation type="jsonpath">$.basic.lists[2].content[1].moreList[0].content[0].text</operation>
				</field-extractor>
				<field-extractor id="tradeNumber" field="tradeNumber" stand-by="true" result-type="string" not-empty="true">
					<operation type="template">${tradeNumberInput}</operation>
				</field-extractor>
				<field-extractor id="tradeDatetime" field="tradeDatetime" result-type="date" stand-by="true" format="yyyy-MM-dd HH:mm:ss">
					<operation type="jsonpath">$.basic.lists[2].content[1].moreList[1].content[0].text</operation>
				</field-extractor>
				<field-extractor id="tradeDatetime" field="tradeDatetime" result-type="date" stand-by="true" format="yyyy-MM-dd HH:mm:ss">
					<operation type="template">${tradeDatetime}</operation>
				</field-extractor>
				<field-extractor id="tradeDatetime" field="tradeDatetime" result-type="date" format="yyyyMMdd" stand-by="true" not-empty="true">
					<operation type="template">${tradeDatetime}</operation>
				</field-extractor>
				<field-extractor id="title" field="title" result-type="string">
					<operation type="template">${title}</operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string" stand-by="true" not-empty="true">
					<operation type="jsonpath">$.basic.lists[0].content[0].text</operation>
				</field-extractor>
			</object-segment>
		</page-extractor>


		<page-extractor id="page-tradeAddress_page" dis-alternative="true">
			<page-sources>
				<source field="pageContent"></source>
			</page-sources>
			<object-segment name="tradeAddress">
				<field-extractor id="tradeNumber" field="tradeNumber" result-type="string">
					<operation type="xpath">div.alipay-number span:eq(1)/text()</operation>
				</field-extractor>
				<field-extractor id="tradeNumber" field="tradeNumber" stand-by="true" result-type="string">
					<operation type="xpath">td:contains(支付宝交易号):not(:has(td:contains(支付宝交易号))) + td/text()</operation>
				</field-extractor>
				<field-extractor id="tradeNumber" field="tradeNumber" stand-by="true" result-type="string">
					<operation type="xpath">span:contains(支付宝交易号):not(:has(span:contains(支付宝交易号))) + span/text()</operation>
				</field-extractor>
				<field-extractor id="tradeNumber" field="tradeNumber" stand-by="true" result-type="string">
					<operation type="xpath">dt:contains(支付宝交易号):not(:has(dt:contains(支付宝交易号))) + dd/text()</operation>
				</field-extractor>
				<field-extractor id="tradeNumber" field="tradeNumber" stand-by="true" result-type="string" not-empty="true">
					<operation type="template">${tradeNumberInput}</operation>
				</field-extractor>
				<field-extractor id="tradeDatetime" field="tradeDatetime" result-type="date" format="yyyy-MM-dd HH:mm:ss">
					<operation type="template">${tradeDatetime}</operation>
				</field-extractor>
				<field-extractor id="tradeDatetime" field="tradeDatetime" result-type="date" format="yyyyMMdd" stand-by="true" not-empty="true">
					<operation type="template">${tradeDatetime}</operation>
				</field-extractor>
				<field-extractor id="title" field="title" result-type="string">
					<operation type="template">${title}</operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string">
					<operation type="xpath">div:contains(收货地址):not(:has(div)) span.address/text()</operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string" stand-by="true">
					<operation type="xpath">div:contains(收货地址):not(:has(div)) span:eq(1)/text()</operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string" stand-by="true">
					<operation type="xpath">li#receiver-info p/text()</operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string" stand-by="true">
					<operation type="xpath">//div[@class='addr_and_note']//dd[1]/text()</operation>
					<operation type="replace" from="\r|\n" to=""></operation>
					<operation type="trim"></operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string" stand-by="true">
					<operation type="xpath">//div[@class='logistics']/div[2]/span[2]/text()</operation>
					<operation type="replace" from="\r|\n" to=""></operation>
					<operation type="trim"></operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string" stand-by="true">
					<operation type="xpath">//table[@class='simple-list logistics-list']/tbody/tr[3]/td[2]/text()</operation>
					<operation type="replace" from="\r|\n" to=""></operation>
					<operation type="trim"></operation>
				</field-extractor>
				<field-extractor id="address" field="address" result-type="string" stand-by="true" not-empty="true">
					<operation type="xpath">//div[@class='address-detail']/text()</operation>
					<operation type="replace" from="\r|\n" to=""></operation>
					<operation type="trim"></operation>
				</field-extractor>
			</object-segment>
		</page-extractor>

		<page-extractor id="page-taobao_record" dis-alternative="true">
			<page-sources>
				<source field="pageContent"></source>
			</page-sources>
			<object-segment name="trades" type="jsonpath" value="$.mainOrders[*]">
				<field-extractor id="tradeDetailUrl" field="tradeDetailUrl" result-type="string">
					<operation type="jsonpath">$.statusInfo.url</operation>
					<operation type="append" index="0"><![CDATA[http:]]></operation>
				</field-extractor>
				<field-extractor id="tradeDetailUrl" field="tradeDetailUrl" result-type="string" stand-by="true">
					<operation type="jsonpath">$.statusInfo.operations.url</operation>
					<operation type="append" index="0"><![CDATA[http:]]></operation>
				</field-extractor>
				<field-extractor id="title" field="title" result-type="string" not-empty="true">
					<operation type="jsonpath">$.subOrders[0].itemInfo.title</operation>
				</field-extractor>
				<field-extractor id="amount" field="amount" result-type="number">
					<operation type="jsonpath">$.payInfo.actualFee</operation>
					<operation type="append" index="0"><![CDATA[-]]></operation>
				</field-extractor>
				<field-extractor id="tradeNumber" field="tradeNumber" result-type="string" not-empty="true">
					<operation type="jsonpath">$.orderInfo.id</operation>
				</field-extractor>
				<field-extractor id="uniqueSign" field="uniqueSign" result-type="string">
					<operation type="template">${tradeNumber}</operation>
				</field-extractor>
				<field-extractor id="tradeDatetime" field="tradeDatetime" result-type="date" format="yyyy-MM-dd HH:mm:ss" not-empty="true">
					<operation type="jsonpath">$.orderInfo.createTime</operation>
				</field-extractor>
				<field-extractor id="otherSide" field="otherSide" result-type="string">
					<operation type="jsonpath">$.seller.shopName</operation>
				</field-extractor>
				<field-extractor id="otherSide" field="otherSide" result-type="string" stand-by="true" not-empty="true">
					<operation type="jsonpath">$.seller.nick</operation>
				</field-extractor>
				<field-extractor id="otherSideName" field="otherSideName" result-type="string">
					<operation type="jsonpath">$.seller.nick</operation>
				</field-extractor>
				<field-extractor id="tradeType" field="tradeType" result-type="int" default-value="0">
					<operation type="set"><![CDATA[SHOPPINGT]]></operation>
					<operation type="mapping" group-name="tradeType"></operation>
				</field-extractor>
				<field-extractor id="tradeStatus" field="tradeStatus" result-type="int" default-value="-1">
					<operation type="jsonpath">$.statusInfo.text</operation>
					<operation type="trim"></operation>
					<operation type="mapping" group-name="tradeStatus"></operation>
				</field-extractor>
			</object-segment>
		</page-extractor>



		<page-extractor id="page-extractor_baseInfo" dis-alternative="true">
			<page-sources>
				<source field="pageContent"></source>
			</page-sources>

			<object-segment name="baseInfo" pop-return="true">
				<field-extractor id="alipayAccount" field="alipayAccount" result-type="string">
					<operation type="regex" index="1"><![CDATA[email\s*:\s*'([^']+)',]]></operation>
				</field-extractor>
				<field-extractor id="alipayAccount" field="alipayAccount" result-type="string" stand-by="true">
					<operation type="xpath">div.pane-title/text()</operation>
					<operation type="regex" index="1"><![CDATA[账户名：\s+([^\s]+)]]></operation>
				</field-extractor>
				<field-extractor id="alipayAccount" field="alipayAccount" result-type="string" stand-by="true">
					<operation type="regex" index="1"><![CDATA[email\s*:\s*"([^"]+)",]]></operation>
				</field-extractor>
				<field-extractor id="alipayAccount" field="alipayAccount" result-type="string" stand-by="true">
					<operation type="xpath">div.pane-title/text()</operation>
					<operation type="regex" index="1"><![CDATA[账户名：\s*([^\s]+)]]></operation>
				</field-extractor>
				<field-extractor id="alipayAccount" field="alipayAccount" result-type="string" stand-by="true" not-empty="true">
					<operation type="xpath">//h3[@id='global-username']/text()</operation>
					<operation type="regex" index="1"><![CDATA[([^\s]+)]]></operation>
				</field-extractor>

				<field-extractor id="name" field="name" result-type="string">
					<operation type="regex" index="1"><![CDATA[userName\s*:\s*'([^']+)',]]></operation>
				</field-extractor>
				<field-extractor id="name" field="name" result-type="string" stand-by="true">
					<operation type="regex" index="1"><![CDATA[userName:"([^"]*)"]]></operation>
				</field-extractor>
				<field-extractor id="name" field="name" result-type="string" stand-by="true">
					<operation type="xpath">.info-table/text()</operation>
					<operation type="regex" index="1"><![CDATA[注册人姓名：\s*([^\s]+)]]></operation>
				</field-extractor>
				<field-extractor id="name" field="name" result-type="string" stand-by="true" not-empty="true">
					<operation type="regex" index="1"><![CDATA[您好，([^。]+)。]]></operation>
				</field-extractor>

				<field-extractor id="email" field="email" result-type="string">
					<operation type="regex" index="1"><![CDATA[email\s*:\s*'([^']+)',]]></operation>
				</field-extractor>
				<field-extractor id="email" field="email" result-type="string" stand-by="true">
					<operation type="regex" index="1"><![CDATA[email\s*:\s*"([^"]+)",]]></operation>
				</field-extractor>
				<field-extractor id="email" field="email" result-type="string" stand-by="true">
					<operation type="xpath">.info-table/text()</operation>
					<operation type="regex" index="1"><![CDATA[Email账户名：\s*([^\s]+)]]></operation>
				</field-extractor>

				<field-extractor id="mobile" field="mobile" result-type="string">
					<operation type="regex" index="1"><![CDATA[mobile\s*:\s*'([^']+)',]]></operation>
				</field-extractor>
				<field-extractor id="mobile" field="mobile" result-type="string" stand-by="true">
					<operation type="regex" index="1"><![CDATA[mobile\s*:\s*"([^"]+)"]]></operation>
				</field-extractor>
				<field-extractor id="mobile" field="mobile" result-type="string" stand-by="true">
					<operation type="xpath">.info-table/text()</operation>
					<operation type="regex" index="1"><![CDATA[绑定手机：\s*([^\s&]+)]]></operation>
				</field-extractor>

				<field-extractor id="alipayBalance" field="alipayBalance" result-type="number">
					<operation type="xpath">//li[@id='J-balance']//p/text()</operation>
					<operation type="replace" from="\n" to=""></operation>
					<operation type="replace" from=" " to=""></operation>
				</field-extractor>
				<field-extractor id="alipayBalance" field="alipayBalance" result-type="number" stand-by="true">
					<operation type="xpath">div.i-assets-balance-amount/text()</operation>
					<operation type="replace" from="\n" to=""></operation>
					<operation type="replace" from=" " to=""></operation>
				</field-extractor>
				<field-extractor id="alipayBalance" field="alipayBalance" result-type="number" not-empty="true" default-value="0" stand-by="true">
					<operation type="xpath">//em[@class='aside-available-amount']/text()</operation>
					<operation type="regex" index="1"><![CDATA[(.+)]]></operation>
					<operation type="trim"></operation>
				</field-extractor>

				<field-extractor id="yuebaoBalance" field="yuebaoBalance" result-type="number">
					<operation type="xpath" index="1">//input[@id='J-mfund-balance']/@value</operation>
					<operation type="regex" index="1"><![CDATA[(.+)]]></operation>
				</field-extractor>
				<field-extractor id="yuebaoBalance" field="yuebaoBalance" result-type="number" stand-by="true">
					<operation type="xpath" index="1">//span[@class='detail-value']//em/text()</operation>
					<operation type="trim"></operation>
				</field-extractor>
				<field-extractor id="yuebaoBalance" field="yuebaoBalance" result-type="number" stand-by="true">
					<operation type="xpath" index="1">//p[@id='J-assets-mfund-amount']/text()</operation>
					<operation type="trim"></operation>
				</field-extractor>

				<field-extractor id="taoBaoAccount" field="taoBaoAccount" result-type="string">
					<operation type="regex" index="1"><![CDATA[<th>淘宝会员名</th>\s*<td>([^<]+)<]]></operation>
				</field-extractor>
				<field-extractor id="taoBaoAccount" field="taoBaoAccount" result-type="string" not-empty="true" stand-by="true">
					<operation type="xpath"><![CDATA[div.personal-info dt:contains(会员名) + dd/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[\s*(.+)]]></operation>
					<operation type="trim"></operation>
				</field-extractor>

				<field-extractor id="alipayRegistrationDatetime" field="alipayRegistrationDatetime" result-type="date" format="yyyy年MM月dd日">
					<operation type="regex" index="1"><![CDATA[<th>注册时间</th>\s*<td>([^<]+)<]]></operation>
				</field-extractor>
				<field-extractor id="alipayRegistrationDatetime" field="alipayRegistrationDatetime" result-type="date" format="yyyy-MM-dd" stand-by="true">
					<operation type="xpath"><![CDATA[th:contains(注册时间) + td/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[([\d\-]+)]]></operation>
				</field-extractor>

				<field-extractor id="isVerified" field="isVerified" result-type="string">
					<operation type="xpath">//table[@class='table-list']//span[4]/text()</operation>
					<operation type="replace" from="待确认" to="0"></operation>
					<operation type="replace" from="已认证" to="1"></operation>
					<operation type="regex" index="1">(\d)</operation>
				</field-extractor>
				<field-extractor id="isVerified" field="isVerified" result-type="string" not-empty="true" stand-by="true">
					<operation type="xpath" empty-to-null="true">//td[@class='fn-middle']/text()</operation>
					<operation type="triple" triple-type="regex">${this} matches:实名认证会员?1:0</operation>
					<operation type="regex" index="1">.*(\d+)</operation>
				</field-extractor>

				<field-extractor id="huabeiAmount" field="huabeiAmount" result-type="number">
					<operation type="regex" index="1"><![CDATA[消费额度：([\d\.]+)\s*元]]></operation>
				</field-extractor>
				<field-extractor id="huabeiBalance" field="huabeiBalance" result-type="number">
					<operation type="regex" index="1"><![CDATA[可用额度：([\d\.]+)\s*元]]></operation>
				</field-extractor>
				<field-extractor id="huabeiAmount" field="huabeiAmount" stand-by="true" result-type="number">
					<operation type="xpath"><![CDATA[div.i-assets-header:contains(蚂蚁花呗) ~ div.i-assets-body p:contains(消费额度)/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[消费额度:\s*([\d\.]+)\s*元]]></operation>
				</field-extractor>
				<field-extractor id="huabeiBalance" field="huabeiBalance" stand-by="true" result-type="number">
					<operation type="xpath"><![CDATA[div.i-assets-header:contains(蚂蚁花呗) ~ div.i-assets-body p:contains(可用额度)/text()]]></operation>
					<operation type="regex" index="1"><![CDATA[可用额度:\s*([\d\.]+)\s*元]]></operation>
				</field-extractor>
				<field-extractor id="huabeiAmount" field="huabeiAmount" stand-by="true" result-type="number">
					<operation type="xpath">div.i-assets-header:contains(蚂蚁花呗) + div.i-assets-body .amount/text()</operation>
